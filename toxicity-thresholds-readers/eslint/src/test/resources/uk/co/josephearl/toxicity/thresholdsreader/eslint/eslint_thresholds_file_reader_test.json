{
  "env": {
    "browser": true
  },
  "rules": {
    "max-statements": [
      "error",
      {
        "max": 30
      }
    ],
    "max-params": [
      "error",
      {
        "max": 6
      }
    ],
    "complexity": [
      "error",
      {
        "max": 10
      }
    ],
    "max-depth": [
      "error",
      {
        "max": 3
      }
    ],
    "default-case": [
      "error"
    ],
    "no-fallthrough": [
      "error"
    ]
  }
}
